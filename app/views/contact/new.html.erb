<%= form_with scope: :contact, url: contact_index_path, method: :post, local: true, class: "container" do |f| %>
  <div class="form-group">
    <%= f.label :prenom %>
    <%= f.text_field :prenom, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :nom %>
    <%= f.text_field :nom, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :message %>
    <%= f.text_area :message, class: 'form-control' %>
  </div>
    <%= f.submit 'Envoyer', class: 'btn btn-primary' %>
<% end %>
